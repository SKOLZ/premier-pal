#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules/biome/dist/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules/biome/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules/biome/dist/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules/biome/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/biome@0.3.3/node_modules:/home/skolz/projects/premier-pal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
